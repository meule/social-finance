        <li>
            <p class="number">
                {{ impact_bonds }}
            </p>
            <p class="text">
                IMPACT BONDS
            </p>
        </li>
        <li>
            <p class="number">
                <span class="symbol">$</span>{{ capital_raised_m }}<span>M</span>
