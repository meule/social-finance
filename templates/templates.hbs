        </li>
    </ul>
    <div class="chart-wrap">
        <div class="chart"></div>
        <h5></h5>
        <div class="description"></div>
    </div>
    <div class="news-list">
        <h3>LATEST NEWS</h3>
        {{#each recent}}
	        <div class="news-item">
	            <h4>{{ project_name }}</h4>
	            <div class="date">{{ launch_date }}</div>
	            <a href="#">LEARN MORE</a>
	            <div class="text">{{ project_overview }}</div>
	        </div>
	    {{/each}}
    </div>
